{
  "name": "gridjs-buttons",
  "version": "1.0.2",
  "description": "Button plugin for GridJS",
  "main": "dist/intex.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "ts-compile": "tsc",
    "sass-compile": "sass theme/main.scss dist/theme/gridjs-buttons.css",
    "build": "npm run sass-compile && npm run ts-compile"
  },
  "keywords": [
    "gridjs",
    "gridjs plugin",
    "table",
    "js table"
  ],
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "author": "kaanrkaan",
  "license": "MIT",
  "devDependencies": {
    "sass": "^1.49.11",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "gridjs": "^5.0.2"
  },
  "repository": "https://github.com/kaanrkaan/gridjs-buttons"
}
